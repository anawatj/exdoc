<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
		<class name="com.tao.exdoc.domain.master.DocumentMode" table="MAS_M_DOCUMENT_MODE">
					<id name="id" column="ID" type="integer">
					<generator class="sequence">
							<param name="sequence">MAS_M_DOCUMENT_MODE_SEQ</param>
					</generator>
				</id>
				<property name="code" column="DOCUMENT_MODE_CODE" length="20"></property>
				<property name="description" column="DOCUMENT_MODE_DESC" length="200"></property>
				<property name="createdBy" column="CREATED_BY" length="20"></property>
				<property name="createdDate" column="CREATED_DATE" ></property>
				<property name="updatedBy" column="UPDATED_BY" length="20"></property>
				<property name="updatedDate" column="UPDATED_DATE"></property>
		</class>
</hibernate-mapping>